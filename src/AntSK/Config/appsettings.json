{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "urls": "http://*:80",
  "ProSettings": {
    "NavTheme": "light",
    "Layout": "side",
    "ContentWidth": "Fluid",
    "FixedHeader": false,
    "FixSiderbar": true,
    "Title": "AntSK",
    "PrimaryColor": "daybreak",
    "ColorWeak": false,
    "SplitMenus": false,
    "HeaderRender": true,
    "FooterRender": true,
    "MenuRender": true,
    "MenuHeaderRender": true,
    "HeaderHeight": 48
  },
  "DBConnection": {
    "DbType": "Sqlite",
    "ConnectionStrings": "Data Source=/app/Config/AntSK.db;"
  },
  "KernelMemory": {
    "VectorDb": "Disk",
    "ConnectionString": "Host=;Port=;Database=antsk;Username=;Password=",
    "TableNamePrefix": "km-"
  },
  "LLamaSharp": {
    "RunType": "CPU"
  },
  "FileDir": {
    "DirectoryPath": "/app/Config/model"
  },
  "Login": {
    "User": "admin",
    "Password": "utp@2018"
  },
  "BackgroundTaskBroker": {
    "ImportKMSTask": {
      "WorkerCount": 1
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      //{
      //  "Name": "OpenTelemetry",
      //  "Args": {
      //    "Endpoint": "http://aspire-dashboard:18889"
      //  }
      //},
      {
        "Name": "File",
        "Args": {
          "path": "/app/Config/logs/log-.txt",
          "rollingInterval": "Day",
          "fileSizeLimitBytes": 10485760
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  }
}
