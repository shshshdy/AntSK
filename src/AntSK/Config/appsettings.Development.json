{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "urls": "http://*:5000",
  "DBConnection": {
    "DbType": "Sqlite",
    "ConnectionStrings": "Data Source=Config\\AntSK.db;"
  },
  "LLamaSharp": {
    "RunType": "CPU"
  },
  "FileDir": {
    "DirectoryPath": "D:\\model"
  },
  "Login": {
    "User": "admin",
    "Password": "ssd"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "D:\\model\\logs\\log-.txt",
          "rollingInterval": "Day",
          "fileSizeLimitBytes": 10485760
        }
      }
    ]
  }
}
